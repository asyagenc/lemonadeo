<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFDBFFF5"
    tools:context=".MathLearning">

    <ImageView
        android:id="@+id/table"
        android:layout_width="450dp"
        android:layout_height="551dp"
        app:srcCompat="@drawable/multip"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="-15dp" />

    <Button
        android:id="@+id/backMathLearning"
        android:layout_width="100dp"
        android:layout_height="65dp"
        android:layout_below="@id/table"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="20dp"
        android:backgroundTint="#FF8BC34A"
        android:fontFamily="@font/crushed"
        android:text="Learning Menu" />

</RelativeLayout>